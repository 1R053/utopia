{
  "scripts": {
    "start": "SNOWPACK_PUBLIC_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID SNOWPACK_PUBLIC_AUTH0_ENDPOINT=$AUTH0_ENDPOINT SNOWPACK_PUBLIC_AUTH0_REDIRECT_URI=$AUTH0_REDIRECT_URI SNOWPACK_PUBLIC_COMMIT_HASH=`git rev-parse HEAD` snowpack dev",
    "build": "if [ \"$GITHUB_SHA\" = \"\" ]; then echo \"ERROR: Missing GITHUB_SHA\" && exit 1; else npm run actually-build; fi",
    "actually-build": "SNOWPACK_PUBLIC_ENVIRONMENT_CONFIG='production' SNOWPACK_PUBLIC_AUTH0_CLIENT_ID=$AUTH0_CLIENT_ID SNOWPACK_PUBLIC_AUTH0_ENDPOINT=$AUTH0_ENDPOINT SNOWPACK_PUBLIC_AUTH0_REDIRECT_URI=$AUTH0_REDIRECT_URI SNOWPACK_PUBLIC_COMMIT_HASH=`git rev-parse HEAD` snowpack build",
    "test": "jest",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "facepaint": "1.2.1",
    "localforage": "1.7.4",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-router-dom": "5.2.0",
    "timeago.js": "4.0.2",
    "url-join": "4.0.1"
  },
  "devDependencies": {
    "@snowpack/app-scripts-react": "1.6.0-alpha.0",
    "@testing-library/jest-dom": "5.11.1",
    "@testing-library/react": "10.4.7",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/url-join": "4.0.0",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "snowpack": "2.6.4",
    "typescript": "3.9.6"
  }
}
